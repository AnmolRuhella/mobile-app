{
  "extends": "expo/tsconfig.base",
"compilerOptions": {
  "target": "esnext",
  "module": "NodeNext",
  "moduleResolution": "nodenext",
  "jsx": "react-jsx",
  "strict": true,
  "allowJs": true,
  "skipLibCheck": true,
  "esModuleInterop": true,
  "forceConsistentCasingInFileNames": true,
  "noEmit": true,
  "baseUrl": ".",
  "paths": {
    "@/*": ["./*"]
  }
},
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
